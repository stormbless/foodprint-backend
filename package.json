{
  "name": "myapp",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "npm run build && nodemon ./dist/app.js"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src"
    ],
    "ext": "ts,js,json",
    "exec": "npm run build && node ./dist/app.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "^4.22.1",
    "helmet": "^8.1.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^7.0.0",
    "morgan": "^1.10.1",
    "ms": "^2.1.3",
    "mysql": "^2.18.1",
    "mysql2": "^3.16.0",
    "nodemon": "^3.1.11",
    "validator": "^13.15.26"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mysql": "^2.15.27",
    "@types/node": "^24.10.4",
    "@types/validator": "^13.15.10",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
